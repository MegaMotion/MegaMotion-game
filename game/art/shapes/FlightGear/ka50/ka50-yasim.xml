           fx="1" fy="0" fz="0"
           ccw="1"
           mincollective="-100" maxcollective="100"
           mincyclicele="-100" maxcyclicele="100"
           mincyclicail="-100" maxcyclicail="100"
           diameter="14.5"
           numblades="3"       
