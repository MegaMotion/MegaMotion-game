<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>interior.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>interieur</object-name>
    <object-name>tour</object-name>

    <!-- Objets transparents -->
    <object-name>intvitres</object-name>
  </animation>

  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Aircraft/Ka-50/Models/Effects/Glass/glass</inherits-from>
    <object-name>intvitres</object-name>
  </effect>
  
  <!-- Planche de bord -->
  <model>
    <path>Aircraft/Ka-50/Models/Interior/Panel/panel.xml</path>
    <offsets>
      <x-m> -2.653 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.206 </z-m>
    </offsets>
  </model>

  <!-- Pilote -->
  <model>
    <path>Aircraft/Ka-50/Models/Pilot/pilot.xml</path>
    <offsets>
      <x-m> -2.165 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.089 </z-m>
    </offsets>
  </model>

</PropertyList>
